{
  "name": "taro-bluetooth-print",
  "version": "1.0.9",
  "description": "基于 Taro 的蓝牙打印机库，支持微信小程序平台的蓝牙打印功能",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.umd.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "rimraf dist coverage docs/api test-results .jest-cache",
    "lint": "eslint 'src/**/*.{js,ts}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "prebuild": "npm run clean",
    "build": "vite build && npm run build:types",
    "build:vite": "vite build",
    "build:webpack": "webpack --config build/webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:api": "node scripts/generate-api-docs.js",
    "docs:full": "npm run docs:build && node scripts/post-build.js",
    "docs:verify": "node scripts/verify-deployment.js",
    "docs:serve": "npx http-server docs/.vitepress/dist -p 5173 -c-1",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:build": "npm run build && npm run test",
    "test:performance": "jest --testPathPattern=performance",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:clear": "jest --clearCache",
    "test:silent": "jest --silent",
    "analyze": "node scripts/bundle-analyzer.js",
    "perf:build": "node scripts/build-performance.js",
    "validate": "node scripts/validate-build.js",
    "quality:check": "node scripts/quality-gate.js --config=quality-gate.config.json",
    "quality:fix": "npm run format && npm run lint --fix",
    "quality:coverage": "node scripts/quality-gate.js --check-type=coverage",
    "quality:security": "node scripts/quality-gate.js --check-type=security",
    "quality:performance": "node scripts/quality-gate.js --check-type=performance",
    "coverage:monitor": "./scripts/coverage-monitor.sh",
    "coverage:report": "node scripts/generate-coverage-report.js --project-name 'Taro Bluetooth Print'",
    "coverage:badge": "./scripts/coverage-monitor.sh --badge",
    "coverage:trend": "./scripts/coverage-monitor.sh --trend",
    "coverage:upload": "./scripts/coverage-monitor.sh --upload",
    "coverage:watch": "./scripts/coverage-monitor.sh --watch",
    "coverage:check": "./scripts/coverage-monitor.sh --check-thresholds",
    "ci:quality": "node scripts/quality-gate.js",
    "ci:build": "npm run quality:check && npm run test && npm run build",
    "ci:full": "npm run lint && npm run test:ci && npm run build && npm run ci:quality",
    "report:test": "node scripts/generate-test-report.js",
    "report:full": "npm run test:ci && npm run coverage:report && npm run report:test",
    "release:prepare": "node scripts/prepare-release.js",
    "release:validate": "node scripts/validate-release.js"
  },
  "keywords": [
    "taro",
    "bluetooth",
    "printer",
    "weapp",
    "mini-program"
  ],
  "author": "Agions",
  "license": "MIT",
  "dependencies": {
    "@tarojs/taro": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/jest-native": "^5.4.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.7.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.0",
    "terser": "^5.24.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-dts": "^3.9.1",
    "vitepress": "^1.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0",
    "ts-loader": "^9.5.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "coveralls": "^3.1.1"
  },
  "sideEffects": false
}